    "electron-log": "^2.2.6",
    "menubar": "^5.2.3",
    "mobx": "^5.6.0",
    "mobx-react": "^5.4.2",
    "moment": "^2.22.2",
    "node-osascript": "^2.1.0",
    "python-shell": "^1.0.6",
    "react": "16.4.0",
    "react-dnd": "^6.0.0",
    "react-dnd-html5-backend": "^6.0.0",
    "react-dom": "16.4.0",
    "react-json-editor": "^0.3.0",
    "react-quill": "^1.3.2",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
